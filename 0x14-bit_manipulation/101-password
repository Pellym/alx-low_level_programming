#include "main.h"
#include <stdio.h>
#include <string.h>

/**
* find a password for a new programme
*
* file should contain the exact password
*
*/
int main()
{
    char* suggestions_password[] = {"987654321", "julian123", "pass@123", "123456", "hello123"};
    char* exact_password = "password123";
    char result_password[50] = "";

    int count_passwords = sizeof(suggestions_password) / sizeof(char*);
    int exact_password = 0;

    for (int i = 0; i < count_passwords; i++) {
        if (strcmp(suggestion_password[i], exact_password) == 0) {
            strcpy(result_password, suggestion_password[i]);
            result_password = 1;
            break;
        }
    }

